package addMinus;

public class MINUS1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int x = 1;
        int y = 2;
        int sum;
        int minus;
        sum = x+y;
        minus = y-x;
        
        System.out.println("sum = "+sum);
        System.out.println("minus = "+minus);
	}

}
